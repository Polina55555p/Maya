ms-center\">\n            <template v-if=\"!check\">\n            <img src=\"@/assets/img/header/phone-dark.svg\" alt=\"\">\n          </template>\n          <template v-else>\n            <img src=\"@/assets/img/header/phone.svg\" alt=\"\">\n          </template>  \n            <div :class='(check ? \"\" : \"dark\")' class=\"text-nowrap ps-3\"> 8 (800) 233-44-55</div>\n         </a>\n        </div>\n    </nav>\n  </div>\n</template>\n\n<script>\nimport { RouterLink } from 'vue-router';\n\nexport default {\n    name: \"Header\",\n    data() {\n        return {\n            headerMenuList: [\n                { name: \"О нас\", path: \"/about\" },\n                { name: \"Каталог\", path: \"/catalog\" },\n                { name: \"Доставка\", path: \"/delivery\" },\n            ],\n            \n\n        }\n\n    },\n    computed: {\n      check() {\n        return this.$route.name === 'home';\n      \n      }\n    }       \n}\n\n</script>\n\n<style scoped lang=\"scss\">\n.header {\n  z-index: 10;\n  padding-top: 100px;\n  position: absolute;\n  color: $lite-text;\n  .navbar {\n  background-color: transparent;\n  .nav-link {\n    color: $lite-text;\n    }\n    .dark {\n    color: $dark-text;\n  }\n  }\n\n}\n.dark {\n  color: $dark-text;\n}\n.lite {\n  color: $lite-text;\n\n}\n\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}